{
  "manifestVersion": 1,
  "author": "Projectum Aps",
  "id": "upload-sp-files-app",
  "name": "SharePoint Files Uploader",
  "version": "1.1.10",
  "publisher": "projectumapsvsts",
  "public": true,
  "content": {
      "details": {
          "path": "overview.md"
      }
  },
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "scopes": [
    "vso.build"
  ],
  "description": "Defines a release task to upload files to SharePoint document library.",
  "categories": [
    "Build and release"
  ],
  "icons": {
    "default": "images/extensionlogo.png"
  },
  "files": [
    {
      "path": "UploadFilesToSPDocLib"
    }
  ],
  "contributions": [
    {
      "id": "upload-sp-task",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "UploadFilesToSPDocLib"
      }
    }
  ]
}